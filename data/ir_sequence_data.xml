<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <record id="eml_expedientes_id_seq" model="ir.sequence">
            <field name="name">Secuencia de Expedientes</field>
            <field name="code">eml.expedientes.sequence</field>
            <field name="prefix">%(y)s-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="eml_control_proveedores_id_seq" model="ir.sequence">
            <field name="name">Secuencia de Folio Control Proveedores</field>
            <field name="code">eml.control.proveedores.sequence</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="eml_transferencias_id_seq" model="ir.sequence">
            <field name="name">Secuencia de Solicitud de Transferencia</field>
            <field name="code">eml.transferencias.sequence</field>
            <field name="prefix">ST-%(y)s-%(month)s-</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False" />
        </record>

    </data>
</odoo>